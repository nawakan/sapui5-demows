<mvc:View controllerName="demows09.controller.Edit"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}"
		showNavButton="true"
		navButtonPress="onNavBack">
        <content>                                    
            <core:Fragment fragmentName="demows09.fragment.HeaderInput" type="XML"/>                  
            <core:Fragment fragmentName="demows09.fragment.ItemInput" type="XML"/>
        </content>
        <footer>                   
            <core:Fragment fragmentName="demows09.fragment.InputFooterToolbar" type="XML"/>
        </footer>
    </Page>
</mvc:View>

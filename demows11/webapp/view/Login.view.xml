<mvc:View controllerName="demows11.controller.Login"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Input id="user" />
            <Input id="password" />
            <Button text="Login" press="onLogin"/>
        </content>
    </Page>
</mvc:View>
